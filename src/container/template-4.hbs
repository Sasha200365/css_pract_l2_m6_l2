

     
<header class='header' >
  <a href="#test">
    {{> logo}}    
  </a>

  <nav class='header_nav'>
    <div class="header_nav_right">
     {{> nav list=header}}
    </div>
  </nav>
</header>



  {{#with main}}
    <div class="main">
      <div class="main_content">
        <div>
         <img class="main_image" src='{{image}}' alt='Main_image'> 
         </div>
         <div>
        <h1 class="main_heading">
          {{heading}}
        </h1>
        <p class="main_description">
          {{description}}
        </p>
        
      
      {{!-- < class="hero_slider">  
        <div class="hero_slider_icon hero_slider_left">{{left}}</div>  --}}
       
        {{!-- <div class="hero_slider_icon hero_slider_right">{{right}}</div>   --}}
       
          <a href="{{href}}" class="button main_button">{{button}}</a>
      
      </div>
      </div> 
    </div>
     
      {{/with}}

 

  {{!-- <div class="modal">
     <div class="modal_taxt">{{hero.text}}{{hero.text}}</div>
     <img class="modal_image" src='{{hero.photo}}' alt='Hero_image'> 
  <div class="modal_text">{{hero.text}}{{hero.text}}</div>
  
</div> --}}

  <div class="featured">
    {{#each featured}}
   {{>card_blog_tmpl4 data=this}}
    {{/each}}
  </div>
<div class="form">
  {{#each actions}}  
      <div class="form_content">
          <h3 class="form_heading">{{heading}}</h3>
          <a href="{{url}}" class="button form_button">{{text}}</a>
      </div>  
  {{/each}}
</div>
   
<footer class="footer" id="test">
  <div>
    {{> logo}}
    <span class='footer_copyright'>Copyright 2023-2024</span>
  </div>        
  {{#each footer}}
    <div>
      <span class="footer_title">
        {{title}}
      </span>
      {{> nav list=links isVertical=true}}
    </div>
  {{/each}}
</footer>

